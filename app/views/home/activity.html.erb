<table class='portfolio'>
  <tr>
    <th>Transaction Type</th>
    <th>Date</th>
	<th>Symbol</th>
	<th>Quantity</th>
	<th>Amount</th>
  </tr>
  <%@activity.each do |n|%>
  <tr class ='portfolio-row'>
	  
    <td class='symbol-cell'>
      <%=n[:transaction_type]%>
    </td>
	
    <td class='symbol-cell'>
      <%=n[:created_at].strftime("%Y-%m-%d")%>
    </td>
	
    <td class='symbol-cell'>
	<%=n[:symbol]%>
	</td>
	
    <td class='symbol-cell'>
	<%=n[:quantity]%>
	</td>
    <td class='symbol-cell'>
		<%=n[:amount]%>
	</td>
  </tr>
  <%end%>
</table>
</div>